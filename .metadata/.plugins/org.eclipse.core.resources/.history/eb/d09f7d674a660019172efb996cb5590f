public class TestMaster 
{
	static WebDriver oWD;
	static String sBr="FF";
	static String sAutUrl="https://www.amazon.in/";
	
	
  @Test
  public void WebSearch() 
  {
	  if(oWD.getCurrentUrl().contains(sAutUrl))
	  {
		  System.out.println("Ready for validation....");
	  }
	  else
		  System.err.println("Navigated to diff URL");
  }
  
  @Test
  public void FindAllLinks() 
  {
	  
  }
  
  
  
  @BeforeClass
  public void beforeClass() 
  {
	  oWD=aut_util.InvokeBrowser(sBr);
	  Assert.assertNotNull(oWD);
	  oWD.navigate().to(sAutUrl);
  }

  @AfterClass
  public void afterClass() 
  {
	  Assert.assertNotNull(oWD);
	  oWD.quit();
	
  }

